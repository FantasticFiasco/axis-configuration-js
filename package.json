{
  "name": "axis-configuration",
  "version": "2.0.0",
  "description": "A node.js library written in TypeScript capable of configuring Axis Communication cameras.",
  "keywords": [
    "axis",
    "communications",
    "camera",
    "device",
    "configuration",
    "vapix"
  ],
  "homepage": "https://github.com/FantasticFiasco/axis-configuration",
  "repository": {
    "type": "git",
    "url": "https://github.com/FantasticFiasco/axis-configuration.git"
  },
  "bugs": {
    "url": "https://github.com/FantasticFiasco/axis-configuration/issues"
  },
  "author": "Mattias Kindborg",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "types": "./lib/index",
  "engines": {
    "node": ">=4.6.1"
  },
  "scripts": {
    "clean": "rimraf \"./+(src|test)/**/*.d.ts\" \"./+(src|test)/**/*.js.map\" \"./+(src|test)/**/*.js\" ./lib ./coverage",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "cpx \"./src/**/*.{d.ts,js}\" ./lib/",
    "prestart": "npm run build",
    "start": "node ./src/server.js",
    "pretest": "npm run build",
    "test": "mocha --recursive",
    "lint": "tslint \"./src/**/*.ts\" --exclude \"./**/*.d.ts\"",
    "precoverage": "npm run build",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec --recursive",
    "postcoverage": "remap-istanbul --input coverage/coverage.json --type lcovonly --output coverage/lcov.info",
    "precoverage:publish": "npm run coverage",
    "coverage:publish": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepack": "npm run test && npm run lint",
    "pack": "npm pack"
  },
  "dependencies": {
    "@fantasticfiasco/expect": "^1.0.0",
    "cheerio": "1.0.0-rc.1",
    "lodash": "4.17.4",
    "request": "2.81.0",
    "request-promise-native": "1.0.4"
  },
  "devDependencies": {
    "@types/chai": "3.5.2",
    "@types/cheerio": "0.22.1",
    "@types/lodash": "4.14.65",
    "@types/mocha": "2.2.41",
    "@types/nock": "8.2.1",
    "@types/node": "8.0.1",
    "@types/request": "0.0.44",
    "@types/request-promise-native": "1.0.5",
    "chai": "4.0.2",
    "coveralls": "2.13.1",
    "cpx": "1.5.0",
    "istanbul": "0.4.5",
    "mocha": "3.4.2",
    "nock": "9.0.13",
    "remap-istanbul": "0.9.5",
    "rimraf": "2.6.1",
    "tslint": "5.4.3",
    "typescript": "2.3.4"
  }
}
