{
  "name": "axis-configuration",
  "version": "2.0.6",
  "description": "A Node.js library written in TypeScript capable of configuring Axis Communication cameras.",
  "keywords": [
    "axis",
    "communications",
    "camera",
    "device",
    "configuration",
    "vapix"
  ],
  "homepage": "https://github.com/FantasticFiasco/axis-configuration",
  "repository": {
    "type": "git",
    "url": "https://github.com/FantasticFiasco/axis-configuration.git"
  },
  "bugs": {
    "url": "https://github.com/FantasticFiasco/axis-configuration/issues"
  },
  "author": "Mattias Kindborg <mattias.kindborg@gmail.com> (https://twitter.com/FantasticFiasco)",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "types": "./lib/index",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rimraf \"./+(src|test)/**/*.+(d.ts|js|js.map)\" ./lib ./coverage",
    "prebuild": "yarn clean",
    "build": "tsc",
    "postbuild": "copyfiles --up 1 \"./src/**/*.{d.ts,js}\" ./lib/",
    "prestart": "yarn build",
    "start": "node ./src/server.js",
    "pretest": "yarn build",
    "test": "mocha --recursive",
    "lint": "tslint \"./src/**/*.ts\" --exclude \"./**/*.d.ts\"",
    "precoverage": "yarn build",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec --recursive",
    "postcoverage": "remap-istanbul --input coverage/coverage.json --type lcovonly --output coverage/lcov.info",
    "precoverage:publish": "yarn coverage",
    "coverage:publish": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "@fantasticfiasco/expect": "1.*",
    "cheerio": "1.0.0-rc.3",
    "lodash": ">4.17.5",
    "request": ">2.68.0",
    "request-promise-native": "1.*"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/cheerio": "0.22.12",
    "@types/lodash": "4.14.136",
    "@types/mocha": "5.2.7",
    "@types/nock": "10.0.3",
    "@types/node": "10.14.13",
    "@types/request-promise-native": "1.0.11",
    "chai": "4.2.0",
    "copyfiles": "2.1.1",
    "coveralls": "3.0.5",
    "istanbul": "0.4.5",
    "mocha": "6.2.0",
    "nock": "10.0.6",
    "remap-istanbul": "0.13.0",
    "rimraf": "2.6.3",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  },
  "resolutions": {
    "diff": ">=3.5.0",
    "handlebars": ">=4.0.14",
    "js-yaml": ">=3.13.1",
    "lodash": ">=4.17.13",
    "sshpk": ">=1.13.2"
  }
}
